# wmbright configuration file

# use mousewheel
mousewheel=1
# scroll the system messages
scrolltext=1
# display OSD
osd=1
# color of the OSD (from rgb.txt or rgb:xx/xx/xx)
osdcolor=green
# which mouse button is "wheel up"
wheelbtn1=4
# which mouse button is "wheel down"
wheelbtn2=5
# the step for mousewheel adjustment
wheelstep=3
